{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "handle": "vercel",
        "src": "/(.*)",
        "methods": ["POST"],
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "http://localhost:3000" },
          { "key": "Access-Control-Allow-Credentials", "value": "true" }
        ]
      }
    ]
  }